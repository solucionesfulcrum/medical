QWidget#videoToolBar {background:transparent}
QWidget#videoToolBar QToolButton{color:#FFF; font-size:13px;  border:0; padding:0;}
QWidget#videoToolBar QToolButton::pressed{background-color:rgba(0,0,0,60); color:#FFF; }


QLabel#VisorListTitle {background-color:#f8f8f8; font-weight:bold; padding:10px 15px;  border-bottom:1px solid #e8e8e8; font-size:18px;}
QLabel#VisorListItem {padding:20px 15px; border-bottom:1px solid #e8e8e8; font-size:16px}
QLabel#VisorListItem[checked="false"]{padding:20px 15px;  background:#ffffff}
QLabel#VisorListItem[checked="true"]{background-color:#00D4D8; color:#FFF; padding:20px 15px; }
QPushButton#visorPlayButton{background-color:#9a9a9a; color:#FFF; }

QSlider#videoSlider::groove:horizontal {
    border: 0px;
    height: 8px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #B1B1B1, stop:1 #c4c4c4);
    margin: 2px 0;
}

QSlider#videoSlider::handle:horizontal {
    background-color:#00D4D8;
    border:0px ;
    width: 16px;
    margin: -4px 0;
    border-radius: 6px;
}
