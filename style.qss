/*Background color */
QMainWindow {background:#ECEDED;}


/*Widget transparent*/
QWidget#transparent{background:transparent; }

/*Default scrollbar */
QScrollBar:vertical {border:0; background:#f0f0f0; width:5px; margin:5px 0 5px 0;}
QScrollBar::handle:vertical {background:#d8d8d8; min-height: 20px; }
QScrollBar::add-line:vertical {border: 0;background:#e0e0e0;height: 5px;subcontrol-position: bottom;subcontrol-origin: margin;}
QScrollBar::sub-line:vertical {border: 0;background:#e0e0e0;height: 5px;subcontrol-position: top;subcontrol-origin: margin;}
QScrollBar::up-arrow:vertical,QScrollBar::down-arrow:vertical {border: 0;width: 2px;height: 2px;background: #3498db ;border-radius:0px;}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical {background: none;}


/* Button */
QPushButton {font-size:16px; background-color:transparent; border:0;  padding:15px 10px;}
QPushButton::pressed { background-color:transparent; border:0;  }
QPushButton::disabled { background-color:transparent; border:0; ;}

QPushButton#redButton,  QPushButton#greenButton {color:#FFF; border-radius:5px;}

QPushButton#redButton {background-color:#FF5454;  }
QPushButton#redButton::disabled { background-color:rgba(200,200,200,255);  color:rgba(255,255,255,255);}

QPushButton#greenButton { background-color:#00D4D8; color:#FFF; }
QPushButton#greenButton::pressed {  background-color:#00D4D8   ; }
QPushButton#greenButton::disabled { background-color:rgba(200,200,200,255);  color:rgba(255,255,255,255);}

QPushButton#blueButton {background-color:#3498db ;  color:#FFF;}
QPushButton#blueButton::pressed { background-color:#1f7bb6; }
QPushButton#blueButton::disabled { background-color:rgba(200,200,200,255);  color:rgba(255,255,255,255);}

QPushButton#yellowButton { background-color:#f1c40f;  color:#FFF;}
QPushButton#yellowButton::pressed { background-color:#e74c3c; }
QPushButton#yellowButton::disabled { background-color:rgba(200,200,200,255);  color:rgba(255,255,255,255);}

/* Message Box */
QMessageBox {background:#272727; border:0; color:#FFF; }
QMessageBox QLabel{color:#FFF; font-size:16px;}
QWidget#footer QMessageBox QLabel{color:#FFF; font-size:14px;}

QMessageBox QPushButton {background-color:#00D4D8;  color:#FFF; padding:10px 20px; border-radius:5px;}


