QWidget#login QPushButton:checked{ background-color:rgba(0,0,0,120); color:#FFF; font-size:14px; padding:15px 10px;  }


QMessageBox QLabel{color:#FFF; font-size:14px;}


/*Default button*/
QPushButton{width:100%; border:0; background-color:rgba(0,0,0,80); color:#FFF; font-size:14px; padding:15px 10px;  }
QPushButton::pressed{width:100%; background-color:rgba(0,0,0,40); color:#FFF; font-size:14px; padding:15px 10px;  }
QPushButton::disabled{width:100%; background-color:rgba(0,0,0,15); color:#FFF; font-size:14px; padding:15px 10px;  }


QPushButton#redButton { background-color:#f35857;  }
QPushButton#redButton::disabled { background-color:rgba(243,88,87,25);}
QPushButton#greenButton { background-color:#52d669;  }
QPushButton#blueButton { background-color:#009699;  }



/*Header*/
QWidget#header{background-color:rgba(0,0,0,200); border-bottom:1px solid #464445;}
QWidget#header QLabel{background-color:rgba(0,0,0,30); color:#FFF; font-size:14px;}
QWidget#header QToolButton{color:#FFF; font-size:13px;  border:0; padding:0;}
QWidget#header QToolButton::pressed{background-color:rgba(0,0,0,200); color:#FFF; border-bottom:1px solid #464445;}
QWidget#header QToolButton::checked{background-color:rgba(0,0,0,200); border-bottom:1px solid #464445; color:#FFF;}
QWidget#header QToolButton#QueueLookButton{ color:#FFF;  }
QWidget#header QToolButton#QueueLookButton::pressed{background-color:rgba(0,0,0,60); color:#FFF;  }
QWidget#header QToolButton#QueueLookButton::checked{background-color:rgba(0,0,0,160);  color:#FFF;  }
QWidget#header QToolButton#menuClose{ color:#FFF;  }

/*Footer*/
