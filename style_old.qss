
QMainWindow,QDialog, QMessageBox, QWidget#dialog {background:#f0f0f0;}

/*
QMainWindow,QDialog, QMessageBox, QWidget#dialog {background:url(':/icon/res/img/background.jpg')}
*/
QWidget#transparent{background:transparent; }

QWidget#dialog QLabel#titleDialog{background-color:rgba(0,0,0,80); color:#FFF; font-size:14px;  padding:15px 10px;}
QWidget#dialog QLabel{background-color:rgba(0,0,0,80); color:#FFF; font-size:14px;  padding:15px 10px;}
QWidget#dialog { border:3px solid #FFF; }

QWidget#login{}
QWidget#login QLabel#block{background-color:rgba(0,0,0,30); color:#FFF; font-size:18px; padding:12px;}
