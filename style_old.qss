QScrollBar:vertical {border:0;background:rgba(0,0,0,120);;width: 50px;margin: 50px 0 50px 0;}
QScrollBar::handle:vertical {background:rgba(0,0,0,210);;min-height: 20px; }
QScrollBar::add-line:vertical {border: 0;background: #33363B;height: 50px;subcontrol-position: bottom;subcontrol-origin: margin;}
QScrollBar::sub-line:vertical {border: 0;background: #33363B;height: 50px;subcontrol-position: top;subcontrol-origin: margin;}
QScrollBar::up-arrow:vertical,QScrollBar::down-arrow:vertical {border: 0;width: 3px;height: 3px;background: white;border-radius:15px;}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical {background: none;}


