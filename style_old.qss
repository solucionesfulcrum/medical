/*Form*/
QWidget#form {}
QWidget#form QLabel{width:100%; background-color:transparent; color:#e0e0e0; font-size:16px; padding:15px 10px; font-weight:bold; }
QWidget#form QPushButton{width:100%; background-color:rgba(0,0,0,80); color:#FFF; font-size:14px; padding:15px 10px;  }
QWidget#form QPushButton::pressed{width:100%; background-color:rgba(0,0,0,40); color:#FFF; font-size:14px; padding:15px 10px;  }

/*Default Editor*/
QLineEdit
,QTimeEdit
,QDateEdit {border:0; padding:15px 10px; color:#FFF; font-size:14px;background-color:rgba(0,0,0,30);}
QWidget#StudyForm QComboBox::drop-down {border:0;}
QWidget#StudyForm QComboBox QAbstractItemView {
    border: 0px solid darkgray;
    background:#363636;
    color:#FFF;
    selection-background-color: #696969;
}

QDateEdit{}
QDateEdit::drop-down {
    image: url(:/icon/res/img/calendar.png);
    width:50px;
    subcontrol-position: right top;
    subcontrol-origin:margin;
    background-color:transparent;
    border-left:1px solid;
    border-color:rgba(0,0,0,10);
}

QDateEdit::drop-down::pressed {
    background-color:rgba(0,0,0,20);
}


QCalendarWidget QWidget{background:url(':/icon/res/img/background.jpg')}
QCalendarWidget QMenu{color:#FFF; background-color:rgba(0,0,0,30); font-size:20px; width: 300px; left: 20px; }
QCalendarWidget QWidget QToolButton {border:0; background:transparent;  color:#FFF; icon-size: 48px, 48px; font: 20px; height: 70px; width: 100px; }
QCalendarWidget QAbstractItemView {selection-background-color: rgb(255, 174, 0);}
QCalendarWidget QToolButton::menu-indicator{ width: 50px;}
QCalendarWidget QToolButton::menu-indicator:pressed,
QCalendarWidget QToolButton::menu-indicator:open{ top:10px; left: 10px;}
QCalendarWidget QTableView { font: 30px; }
QCalendarWidget QPushButton { background-color:#FFF; }
QCalendarWidget QSpinBox {background-color:rgba(0,0,0,30); width: 100px; font: 20px; color:#FFF;}
QCalendarWidget QSpinBox::up-button {  width:40px;}
QCalendarWidget QSpinBox::down-button {  width:40px;}

QLineEdit::disabled
,QTimeEdit::disabled
,QDateEdit::disabled {border:0; padding:15px 10px; color:#999; font-size:14px;background-color:rgba(0,0,0,30);}
QWidget#StudyForm QComboBox::drop-down {border:0;}
QWidget#StudyForm QComboBox QAbstractItemView {
    border: 0px solid darkgray;
    background:#363636;
    color:#FFF;
    selection-background-color: #696969;
}


