QWidget#footer QToolButton::pressed{background-color:rgba(0,0,0,60); color:#FFF; }
QWidget#footer QToolButton::checked{background-color:rgba(0,0,0,200); border:0; color:#FFF;}
QWidget#footer QToolButton#QueueLookButton{ color:#FFF;  }
QWidget#footer QToolButton#QueueLookButton::pressed{background-color:rgba(0,0,0,60); color:#FFF;  }
QWidget#footer QToolButton#QueueLookButton::checked{background-color:rgba(0,0,0,160);  color:#FFF;  }
QWidget#footer QToolButton#menuClose{ color:#FFF;  }

